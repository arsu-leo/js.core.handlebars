{
  "name": "@superhero/core.handlebars",
  "version": "1.7.0",
  "description": "Addon module to add a handlebars template view to the @superhero/core module",
  "repository": "git@github.com:superhero/js.core.handlebars.git",
  "main": "index.js",
  "license": "MIT",
  "author": {
    "name": "Erik Landvall",
    "email": "erik@landvall.se",
    "url": "http://erik.landvall.se"
  },
  "scripts": {
    "prepublishOnly": "npm test && npm run-script generate-doc-coverage && npm run-script generate-doc-tests",
    "generate-doc-coverage": "nyc report --reporter=html --report-dir=./doc/coverage",
    "generate-doc-tests": "mocha './{,!(node_modules)/**}/*.test.js' --reporter mochawesome --reporter-options reportDir=doc/tests,reportFilename=index,showHooks=always",
    "test": "nyc mocha './{,!(node_modules)/**}/*.test.js'"
  },
  "dependencies": {
    "@superhero/debug": "1.1.4",
    "handlebars": "4.0.11",
    "dateformat": "3.0.2"
  },
  "devDependencies": {
    "@superhero/core": "1.7.0",
    "@superhero/syntax-check": "0.0.1",
    "mocha": "5.1.0",
    "mochawesome": "3.0.2",
    "chai": "4.1.2",
    "nyc": "11.7.1"
  }
}
